		"parcel": "^2.13.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.21.0",
		"@parcel/packager-raw-url": "^2.13.3",
		"@parcel/transformer-webmanifest": "^2.13.3",
		"buffer": "^6.0.3",
		"eslint": "^9.21.0",
