{
	"name": "egg-your-books",
	"version": "1.0.0",
	"description": "Rate and track your books with egg ratings",
	"type": "module",
	"scripts": {
		"start": "parcel index.html",
		"build": "parcel build --dist-dir docs --public-url /Egg-Your-Books/ index.html",
